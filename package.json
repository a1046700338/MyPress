{
  "name": "vuepress-theme-hope-template",
  "version": "2.0.0",
  "description": "MyPress is Blog",
  "author": "sakurafeiyu",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "docs:build": "vuepress build src",
    "docs:clean-dev": "vuepress dev src --clean-cache",
    "docs:dev": "vuepress dev src",
    "docs:update-package": "npx vp-update",
    "commit": "git-cz && git push",
    "release": "release-it",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a1046700338/MyPress.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "changelog": {
    "bugsUrl": "https://github.com/a1046700338/MyPress/issues",
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "devDependencies": {
    "@vuepress/client": "2.0.0-beta.61",
    "@vuepress/plugin-register-components": "^2.0.0-beta.61",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "release-it": "^15.10.1",
    "vue": "^3.2.47",
    "vuepress": "2.0.0-beta.61",
    "vuepress-plugin-search-pro": "^2.0.0-beta.200",
    "vuepress-theme-hope": "2.0.0-beta.200"
  }
}
